{
  "name": "@jsreport/jsreport-pdf-utils",
  "version": "3.0.0",
  "description": "jsreport extension providing pdf operations like merge or concatenation",
  "keywords": [
    "jsreport",
    "pdf"
  ],
  "homepage": "https://github.com/jsreport/jsreport/tree/master/packages/jsreport-pdf-utils",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jsreport/jsreport.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jan Blaha"
  },
  "main": "index.js",
  "files": [
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "scripts": {
    "build": "jsreport-studio-build",
    "lint": "standard",
    "prepublishOnly": "jsreport-studio-build",
    "start": "jsreport-studio-start --httpPort 5488",
    "test": "set DEBUG=jsreport&&mocha test --timeout=20000 && standard",
    "test-full": "set DEBUG=jsreport&&set TEST_FULL=true&& mocha test --timeout=20000 && standard"
  },
  "dependencies": {
    "@jsreport/pdfjs": "0.6.0",
    "cheerio": "0.22.0",
    "lodash.omit": "4.5.0",
    "nanoid": "3.1.12",
    "node-forge": "0.10.0",
    "node-signpdf": "1.3.2",
    "node.extend": "2.0.2",
    "parse-color": "1.0.0",
    "pdfjs-dist": "2.0.489"
  },
  "devDependencies": {
    "@jsreport/jsreport-assets": "3.0.0",
    "@jsreport/jsreport-child-templates": "3.0.0",
    "@jsreport/jsreport-chrome-pdf": "3.0.0",
    "@jsreport/jsreport-core": "3.0.0",
    "@jsreport/jsreport-handlebars": "3.0.0",
    "@jsreport/jsreport-jsrender": "3.0.0",
    "@jsreport/jsreport-scripts": "3.0.0",
    "@jsreport/studio-dev": "3.0.0",
    "handlebars": "4.7.7",
    "mocha": "8.2.1",
    "puppeteer": "10.4.0",
    "rimraf": "2.6.3",
    "should": "13.2.3",
    "standard": "16.0.3"
  },
  "engines": {
    "node": ">=16.11"
  },
  "standard": {
    "ignore": [
      "studio/main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  }
}
