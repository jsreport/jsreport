{
  "name": "jsreport-import-export",
  "version": "2.1.1",
  "description": "jsreport extension adding templates exporting and importing API",
  "keywords": [
    "jsreport",
    "export",
    "import"
  ],
  "homepage": "https://github.com/jsreport/jsreport-import-export",
  "repository": {
    "type": "git",
    "url": "git@github.com:jsreport/jsreport-import-export.git"
  },
  "author": "Jan Blaha",
  "files": [
    "cli",
    "lib",
    "studio",
    "index.js",
    "jsreport.config.js"
  ],
  "scripts": {
    "build": "jsreport-studio-build",
    "prepublishOnly": "jsreport-studio-build",
    "prepublish": "in-publish && jsreport-studio-build || not-in-publish",
    "start": "jsreport-studio-start --httpPort 5488",
    "test": "mocha test --timeout 15000 --exit && standard",
    "test:watch": "mocha test --watch"
  },
  "dependencies": {
    "archiver": "5.3.0",
    "axios": "0.21.1",
    "form-data": "2.3.3",
    "lodash.omit": "4.5.0",
    "multer": "1.4.1",
    "p-reduce": "2.1.0",
    "stream-to-array": "2.3.0",
    "yauzl": "2.10.0"
  },
  "devDependencies": {
    "execa": "1.0.0",
    "in-publish": "2.0.1",
    "jsreport-assets": "1.7.0",
    "jsreport-authentication": "2.6.1",
    "jsreport-authorization": "2.4.0",
    "jsreport-cli": "2.2.5",
    "jsreport-core": "2.10.1",
    "jsreport-data": "2.4.0",
    "jsreport-express": "2.8.1",
    "jsreport-fs-store": "2.9.2",
    "jsreport-mongodb-store": "1.3.3",
    "jsreport-postgres-store": "1.4.0",
    "jsreport-studio-dev": "1.6.0",
    "mkdirp": "0.5.5",
    "mocha": "5.0.1",
    "node.extend.without.arrays": "1.1.6",
    "rimraf": "2.6.3",
    "should": "13.2.3",
    "standard": "11.0.0",
    "supertest": "3.4.2"
  },
  "engines": {
    "node": ">=8.9"
  },
  "standard": {
    "ignore": [
      "studio/main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  }
}