    "xml2js-preserve-spaces": "0.0.1"
  },
  "devDependencies": {
    "cross-env": "6.0.3",
    "handlebars": "4.7.7",
    "in-publish": "2.0.1",
    "@jsreport/jsreport-assets": "3.0.0-beta.1",
    "@jsreport/jsreport-core": "3.0.0-beta.1",
