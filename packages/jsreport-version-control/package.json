    "isbinaryfile": "3.0.3",
    "lodash.omit": "4.5.0",
    "mime-types": "2.1.27",
    "nanoid": "3.3.8",
    "node.extend.without.arrays": "1.1.6",
    "p-map": "4.0.0"
  },
  "devDependencies": {
    "@jsreport/jsreport-assets": "4.3.0",
    "@jsreport/jsreport-authentication": "4.3.1",
    "@jsreport/jsreport-chrome-pdf": "4.4.0",
    "@jsreport/jsreport-core": "4.9.0",
    "@jsreport/jsreport-data": "4.2.0",
    "@jsreport/jsreport-express": "4.3.1",
    "@jsreport/studio-dev": "4.1.0",
    "mocha": "11.7.5",
    "puppeteer": "24.31.0",
    "rimraf": "2.6.3",
    "should": "13.2.3",
    "standard": "16.0.4",
    "supertest": "3.4.2"
  },
  "engines": {
    "node": ">=22.18"
  },
  "standard": {
