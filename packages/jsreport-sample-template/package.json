{
  "name": "@jsreport/jsreport-sample-template",
  "version": "4.1.0",
  "description": "jsreport extension which creates a sample template at the first run",
  "keywords": [
    "jsreport"
  ],
  "homepage": "https://github.com/jsreport/jsreport/tree/master/packages/jsreport-sample-template",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jsreport/jsreport.git"
  },
  "license": "LGPL",
  "author": {
    "name": "Jan Blaha"
  },
  "files": [
    "lib",
    "studio",
    "index.js",
    "samples.json",
    "jsreport.config.js"
  ],
  "scripts": {
    "build": "node lib/prepublish.js && jsreport-studio-build",
    "prepublish": "npm run build",
    "start": "jsreport-studio-start --httpPort 4300",
    "test": "npm run build && mocha test --timeout 20000 && standard"
  },
  "dependencies": {
    "lodash.omit": "4.5.0"
  },
  "devDependencies": {
    "@jsreport/jsreport-assets": "4.3.0",
    "@jsreport/jsreport-chrome-pdf": "4.4.0",
    "@jsreport/jsreport-core": "4.9.0",
    "@jsreport/jsreport-data": "4.2.0",
    "@jsreport/jsreport-fs-store": "4.2.0",
    "@jsreport/jsreport-handlebars": "4.1.0",
    "@jsreport/jsreport-html-to-xlsx": "4.4.0",
    "@jsreport/jsreport-pdf-utils": "4.5.3",
    "@jsreport/jsreport-scripts": "4.3.0",
    "@jsreport/jsreport-xlsx": "4.6.1",
    "@jsreport/studio-dev": "4.1.0",
    "handlebars": "4.7.7",
    "mocha": "11.7.5",
    "puppeteer": "24.36.1",
    "rimraf": "^3.0.2",
    "should": "13.2.3",
    "standard": "16.0.4"
  },
  "engines": {
    "node": ">=22.18"
  },
  "standard": {
    "ignore": [
      "samples/**",
      "lib/samples/**",
      "test/data",
      "studio/main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  }
}
