{
  "name": "@jsreport/jsreport-browser-client",
  "version": "4.0.0",
  "description": "jsreport browser client",
  "keywords": [
    "jsreport"
  ],
  "homepage": "https://github.com/jsreport/jsreport/tree/master/packages/jsreport-browser-client",
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/jsreport/jsreport.git"
  },
  "license": "MIT",
  "author": {
    "name": "Jan Blaha"
  },
  "files": [
    "lib",
    "static",
    "index.js",
    "jsreport.config.js",
    "studio"
  ],
  "scripts": {
    "build": "jsreport-studio-build",
    "prepublish": "npm run build",
    "run": "npm run build && jsreport-studio-start --httpPort 5488 --run-only",
    "start": "jsreport-studio-start --httpPort 5488",
    "test": "mocha test --timeout 5000&&standard"
  },
  "dependencies": {
    "@jsreport/browser-client": "3.1.1"
  },
  "devDependencies": {
    "@jsreport/studio-dev": "4.0.0",
    "@jsreport/jsreport-core": "4.0.0",
    "@jsreport/jsreport-express": "4.0.0",
    "@jsreport/jsreport-handlebars": "4.0.0",
    "mocha": "9.1.2",
    "puppeteer": "21.1.0",
    "should": "13.2.3",
    "standard": "16.0.4"
  },
  "engines": {
    "node": ">=18.15"
  },
  "maintainers": [
    {
      "name": "pofider",
      "email": "jan.blaha@hotmail.com"
    }
  ],
  "standard": {
    "ignore": [
      "studio/main.js"
    ],
    "env": {
      "node": true,
      "mocha": true,
      "browser": true
    }
  }
}
